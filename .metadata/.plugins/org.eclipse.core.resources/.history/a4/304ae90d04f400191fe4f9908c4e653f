package lista_polimorfismo;

public class Quadrado extends FormaGeometrica{
	
	private double lado;

	public double getLado() {
		return lado;
	}

	public void setLado(double lado) {
		this.lado = lado;
	}

	public Quadrado(String nome, double lado) {
		super(nome);
		this.lado = lado;
	}

	public Quadrado() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
	

}
